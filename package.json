    "npm-run-all": "^4.1.5",
    "rollup": "^2.52.2",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.35.1",
    "three": "^0.129.0"
  }
}
